{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.DataProtection": "None"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "mobile client",
    "Audience": "web api",
    "MinutesToExpiration": "25"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=172.17.0.8;Database=achieve_club;UserId={{db_user}};Password={{db_password}};",
    "RedisConnection": "172.17.0.5"
  },
  "jwt_key": "{{jwt_key}}",
  "EmailSettings": {
    "Apikey": "{{email_api_key}}",
    "Email": "{{email_address}}",
    "Name": "no-reply"
  },
  "Certificates": {
    "Public": "{{public_key_path}}",
    "Private": "{{public_key_path}}"
  }
}