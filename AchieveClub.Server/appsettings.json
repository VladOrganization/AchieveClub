{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.DataProtection": "Warning",
      "Microsoft.EntityFrameworkCore.Database": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "mobile client",
    "Audience": "web api",
    "MinutesToExpiration": "25"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=172.17.0.4;Database=achieve_club;UserId={{db_user}};Password={{db_user}};",
    "RedisConnection": "172.17.0.3",
    "DashboardConnection": "http://172.17.0.6:18889"
  },
  "jwt_key": "{{jwt_key}}",
  "EmailSettings": {
    "Apikey": "{{email_api_key}}",
    "Email": "{{email}}",
    "Name": "no-reply"
  },
  "Certificates": {
    "Public": "{{public_ssl_path}}",
    "Private": "{{private_ssl_path}}"
  }
}