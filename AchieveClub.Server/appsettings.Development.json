{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.DataProtection": "Warning",
      "Microsoft.EntityFrameworkCore.Database": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "mobile client",
    "Audience": "web api",
    "MinutesToExpiration": "25"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=127.0.0.1;Port=5432;Database=achieve_club;User ID={{db_user}};Password={{db_password}};",
    "RedisConnection": "127.0.0.1:6379",
    "DashboardConnection": "http://localhost:18889"
  },
  "jwt_key": "{{jwt_key}}",
  "EmailSettings": {
    "Apikey": "{{email_api_key}}",
    "Email": "{{email}}",
    "Name": "no-reply"
  }
}
